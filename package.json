{
  "name": "teleapstyle",
  "author": "Nick G.",
  "license": "MIT",
  "version": "0.0.0",
  "description": "Teleapstyle dev",
  "main": "./build/teleapstyle.umd.cjs",
  "types": "./build/index.d.ts",
  "module": "./build/teleapstyle.js",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./build/teleapstyle.js",
        "types": "./build/index.d.ts"
      },
      "require": "./dist/teleapstyle.umd.cjs"
    }
  },
  "type": "module",
  "homepage": "https://psychosynthesis.github.io/Teleapstyle/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Psychosynthesis/Teleapstyle.git"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build && tsc src/index.ts --declaration --emitDeclarationOnly --jsx react-jsx --esModuleInterop --outDir build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "telegram",
    "components"
  ],
  "dependencies": {
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "vanicom": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "clsx": "^2.0.0",
    "gh-pages": "^6.1.1",
    "sass": "^1.69.4",
    "typescript": "^5.0.2",
    "vite": "~4.5.0",
    "vite-plugin-css-injected-by-js": "^3.4.0"
  }
}
